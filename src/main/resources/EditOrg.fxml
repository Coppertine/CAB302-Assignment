<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cab302qut.java.Controller.EditOrgController">
   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0" prefWidth="243.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="454.0" minWidth="10.0" prefWidth="357.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ChoiceBox fx:id="orgAssets" prefWidth="150.0" value="Select Asset" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="editCredits" promptText="Edit Available Credits" GridPane.columnIndex="1" />
            <TextField fx:id="editAssetNum" promptText="Edit Quantity of Assets" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="btn_updateCredits" mnemonicParsing="false" onAction="#SendCreditsUpdate" prefHeight="69.0" prefWidth="66.0" text="Send Credits Update" GridPane.rowIndex="2" />
            <Button fx:id="btn_updateAssets" mnemonicParsing="false" onAction="#SendAssetUpdate" text="Send Asset Update" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
            <Label fx:id="helperLabel" text="cHOSEN ORG" GridPane.rowIndex="1" />
            <Label fx:id="helperLabel2" layoutX="10.0" layoutY="202.0" text="aSSET" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP" />
            <Button mnemonicParsing="false" onAction="#Back" text="Back" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="BOTTOM" />
            <ChoiceBox fx:id="orgChoice" prefWidth="150.0" value="Select Organisation" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
